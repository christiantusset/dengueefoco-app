<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
											 xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
											 android:layout_height="wrap_content">

	<TextView
			android:text="1486"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/textViewId" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"/>
	<TextView
			android:text="Distrito:"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/textViewLabelDistrito" app:layout_constraintTop_toBottomOf="@+id/textViewId" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp" android:layout_marginTop="8dp"/>
	<TextView
			android:text="Data:"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/textViewLabelData" app:layout_constraintEnd_toStartOf="@+id/textViewData" android:layout_marginEnd="8dp" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"/>
	<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/textViewData" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" app:layout_constraintTop_toTopOf="@+id/textViewLabelData" tools:text="10/09/2019"/>
	<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/textViewDistrito" app:layout_constraintStart_toEndOf="@+id/textViewLabelDistrito" android:layout_marginStart="8dp" app:layout_constraintTop_toTopOf="@+id/textViewLabelDistrito" tools:text="Central"/>

	<View
			android:id="@+id/line"
			android:layout_width="0dp"
			android:layout_height="1dp"
			android:layout_marginTop="8dp"
			android:background="@android:color/black"
			android:focusable="false"
			app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/textViewLabelDistrito"/>

</androidx.constraintlayout.widget.ConstraintLayout>
