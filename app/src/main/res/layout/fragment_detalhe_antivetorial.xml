<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
												   xmlns:app="http://schemas.android.com/apk/res-auto"
												   xmlns:tools="http://schemas.android.com/tools"
												   android:layout_width="match_parent"
												   android:layout_height="match_parent">

	<androidx.cardview.widget.CardView
			android:id="@+id/cardView"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_marginStart="8dp"
			android:layout_marginTop="8dp"
			android:layout_marginEnd="8dp"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent">

		<androidx.constraintlayout.widget.ConstraintLayout
				android:layout_width="match_parent"
				android:layout_height="match_parent">

			<TextView
					android:id="@+id/textView4"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginTop="8dp"
					android:text="Larvicida:"
					android:textColor="@color/colorTextoLabel"
					android:textStyle="bold"
					app:layout_constraintStart_toStartOf="parent"
					app:layout_constraintTop_toTopOf="parent" android:layout_marginStart="8dp"/>

			<TextView
					android:id="@+id/textViewLarvicida"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textStyle="bold"
					tools:text="Sinopsade" app:layout_constraintStart_toEndOf="@+id/textView4" android:layout_marginStart="4dp" app:layout_constraintTop_toTopOf="@+id/textView4"/>

			<TextView
					android:id="@+id/textView6"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Qtd. Larvic.:"
					android:textColor="@color/colorTextoLabel"
					android:textStyle="bold"
					app:layout_constraintStart_toStartOf="@+id/textView4"
					app:layout_constraintTop_toBottomOf="@+id/textView4" android:layout_marginTop="8dp"/>

			<TextView
					android:id="@+id/textViewQuantidade"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textStyle="bold"
					tools:text="50" app:layout_constraintStart_toEndOf="@+id/textView6" android:layout_marginStart="4dp" app:layout_constraintTop_toTopOf="@+id/textView6"/>

			<TextView
					android:id="@+id/textView8"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Status:"
					android:textColor="@color/colorTextoLabel"
					android:textStyle="bold"
					android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toStartOf="@+id/textViewLabelStatus" android:layout_marginEnd="4dp"/>

			<TextView
					android:id="@+id/textViewLabelStatus"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textStyle="bold"
					tools:text="Enviado" app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="@+id/textView8" android:layout_marginEnd="8dp"/>

			<TextView
					android:id="@+id/textView10"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Data:"
					android:textColor="@color/colorTextoLabel"
					android:textStyle="bold"
					android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView8" app:layout_constraintEnd_toStartOf="@+id/textViewData" android:layout_marginEnd="4dp"/>

			<TextView
					android:id="@+id/textViewData"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textStyle="bold"
					tools:text="10/08/2019" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" app:layout_constraintTop_toTopOf="@+id/textView10"/>

			<TextView
					android:id="@+id/textView12"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Status do imóvel:"
					android:textColor="@color/colorTextoLabel"
					android:textStyle="bold"
					app:layout_constraintStart_toStartOf="@+id/textView6" android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textViewQuantidade"/>

			<TextView
					android:id="@+id/textViewStatusImovel"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:textStyle="bold"
					tools:text="Visitado" app:layout_constraintTop_toTopOf="@+id/textView12" app:layout_constraintStart_toEndOf="@+id/textView12" android:layout_marginStart="4dp"/>

			<TextView
					android:id="@+id/textView14"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Tipo do imóvel:"
					android:textColor="@color/colorTextoLabel"
					android:textStyle="bold"
					android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView12" app:layout_constraintStart_toStartOf="@+id/textView12" app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"/>

			<TextView
					android:id="@+id/textViewTipoImovel"
					android:layout_width="wrap_content"
					android:textStyle="bold"
					android:layout_height="wrap_content"
					tools:text="Casa" app:layout_constraintTop_toTopOf="@+id/textView14" app:layout_constraintStart_toEndOf="@+id/textView14" android:layout_marginStart="4dp"/>

			<TextView
					android:text="Horário:"
					android:layout_width="wrap_content"
					android:textColor="@color/colorTextoLabel"
					android:textStyle="bold"
					android:layout_height="wrap_content" android:id="@+id/textView3" app:layout_constraintTop_toTopOf="@+id/textViewHorario" app:layout_constraintEnd_toStartOf="@+id/textViewHorario" android:layout_marginEnd="4dp"/>
			<TextView
					android:layout_width="wrap_content"
					android:textStyle="bold"
					android:layout_height="wrap_content" android:id="@+id/textViewHorario" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toBottomOf="@+id/textViewData" android:layout_marginTop="8dp" tools:text="20:30:10"/>

			<TextView
					android:text="Notificado:"
					android:layout_width="wrap_content"
					android:textColor="@color/colorTextoLabel"
					android:textStyle="bold"
					android:layout_height="wrap_content" android:id="@+id/textView7" android:layout_marginEnd="4dp" app:layout_constraintEnd_toStartOf="@+id/textViewBairro" app:layout_constraintTop_toTopOf="@+id/textViewBairro"/>
			<TextView
					android:layout_width="wrap_content"
					android:textStyle="bold"
					android:layout_height="wrap_content" android:id="@+id/textViewBairro" tools:text="Sim" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" app:layout_constraintTop_toBottomOf="@+id/textViewHorario" android:layout_marginTop="8dp"/>

		</androidx.constraintlayout.widget.ConstraintLayout>
	</androidx.cardview.widget.CardView>
	<androidx.cardview.widget.CardView
			android:layout_width="0dp"
			android:layout_height="wrap_content" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp" android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/cardView" android:id="@+id/cardView2">
		<androidx.constraintlayout.widget.ConstraintLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content">

			<TextView
					android:text="Endereço"
					android:textColor="@color/colorTextoLabel"
					android:textStyle="bold"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:id="@+id/textView5" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"/>
			<TextView
					android:text="Logradouro:"
					android:layout_width="wrap_content"
					android:textColor="@color/colorTextoLabel"
					android:textStyle="bold"
					android:layout_height="wrap_content" android:id="@+id/textView9" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp" app:layout_constraintTop_toBottomOf="@+id/textView5"/>
			<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:id="@+id/textViewLogradouro" android:layout_marginStart="4dp" app:layout_constraintStart_toEndOf="@+id/textView9" app:layout_constraintTop_toTopOf="@+id/textView9" tools:text="Rua A" android:textStyle="bold"/>
			<TextView
					android:text="Setor:"
					android:layout_width="wrap_content"
					android:textColor="@color/colorTextoLabel"
					android:textStyle="bold"
					android:layout_height="wrap_content" android:id="@+id/textView13" app:layout_constraintTop_toBottomOf="@+id/textView9" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp" android:layout_marginTop="8dp"/>
			<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:id="@+id/textViewSetor" app:layout_constraintTop_toTopOf="@+id/textView13" app:layout_constraintStart_toEndOf="@+id/textView13" android:layout_marginStart="4dp" tools:text="Central" android:textStyle="bold"/>
			<TextView
					android:text="Quadra:"
					android:layout_width="wrap_content"
					android:textColor="@color/colorTextoLabel"
					android:textStyle="bold"
					android:layout_height="wrap_content" android:id="@+id/textView16" android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toBottomOf="@+id/textView13" android:layout_marginTop="8dp" app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"/>
			<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:id="@+id/textViewQuadra" tools:text="10" app:layout_constraintStart_toEndOf="@+id/textView16" android:layout_marginStart="8dp" app:layout_constraintTop_toTopOf="@+id/textView16" android:textStyle="bold"/>
			<TextView
					android:text="Lote:"
					android:layout_width="wrap_content"
					android:textColor="@color/colorTextoLabel"
					android:textStyle="bold"
					android:layout_height="wrap_content" android:id="@+id/textView18" app:layout_constraintStart_toEndOf="@+id/textViewQuadra" android:layout_marginStart="16dp" app:layout_constraintTop_toTopOf="@+id/textView16"/>
			<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:id="@+id/textViewLote" tools:text="10" app:layout_constraintStart_toEndOf="@+id/textView18" android:layout_marginStart="4dp" app:layout_constraintTop_toTopOf="@+id/textView18" android:textStyle="bold"/>
			<TextView
					android:text="Número:"
					android:textColor="@color/colorTextoLabel"
					android:textStyle="bold"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:id="@+id/textView20" app:layout_constraintStart_toEndOf="@+id/textViewLote" android:layout_marginStart="16dp" app:layout_constraintTop_toTopOf="@+id/textViewLote"/>
			<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:id="@+id/textViewNumero" tools:text="10" app:layout_constraintTop_toTopOf="@+id/textView20" app:layout_constraintStart_toEndOf="@+id/textView20" android:layout_marginStart="4dp" android:textStyle="bold"/>
		</androidx.constraintlayout.widget.ConstraintLayout>
	</androidx.cardview.widget.CardView>
	<androidx.cardview.widget.CardView
			android:layout_width="0dp"
			android:layout_height="wrap_content" app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp" android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/cardView2" android:layout_marginEnd="8dp">
		<LinearLayout
				android:orientation="horizontal"
				android:layout_width="match_parent"
				android:layout_height="match_parent">
			<androidx.constraintlayout.widget.ConstraintLayout
					android:layout_width="match_parent"
					android:layout_height="match_parent">

				<TextView
						android:text="Observacao:"
						android:layout_width="wrap_content"
						android:textColor="@color/colorTextoLabel"
						android:textStyle="bold"
						android:layout_height="wrap_content" android:id="@+id/textView17" app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="8dp" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp" app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"/>
				<TextView
						android:layout_width="0dp"
						android:layout_height="0dp" android:id="@+id/textViewObservacao" app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="8dp" app:layout_constraintStart_toEndOf="@+id/textView17" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent" tools:text="teste" android:textStyle="bold"/>
			</androidx.constraintlayout.widget.ConstraintLayout>
		</LinearLayout>
	</androidx.cardview.widget.CardView>

