<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
											 xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
											 android:layout_height="match_parent">
	<EditText
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:inputType="textPersonName"
			android:text="Agente"
			android:ems="10"
			android:id="@+id/editTextAgente" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"/>
	<Spinner
			android:layout_width="0dp"
			android:autofillHints="Larvicida"
			android:layout_height="wrap_content" android:id="@+id/spinnerLarvicida" android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/editTextAgente" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"/>
	<EditText
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:inputType="textPersonName"
			android:text="Quantidade"
			android:ems="10"
			android:id="@+id/editTextQuantidade" app:layout_constraintTop_toBottomOf="@+id/spinnerLarvicida" android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"/>
	<Spinner
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:id="@+id/spinnerStatusImovel" android:layout_marginTop="8dp"
			app:layout_constraintEnd_toStartOf="@+id/spinnerTipoImovel"
			app:layout_constraintTop_toBottomOf="@+id/editTextQuantidade"
			app:layout_constraintStart_toStartOf="parent"
			android:layout_marginStart="8dp"/>
	<Spinner
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:id="@+id/spinnerTipoImovel"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="8dp"
			android:layout_marginStart="8dp"
			app:layout_constraintStart_toEndOf="@+id/spinnerStatusImovel"
			app:layout_constraintTop_toTopOf="@+id/spinnerStatusImovel"/>
	<EditText
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:inputType="textPersonName"
			android:text="CEP"
			android:ems="10"
			android:id="@+id/editTextCep" app:layout_constraintTop_toBottomOf="@+id/spinnerTipoImovel" android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="8dp" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"/>
	<Button
			android:text="Salvar"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:id="@+id/buttonSalvar"
			android:layout_marginTop="8dp"
			app:layout_constraintTop_toBottomOf="@+id/editTextCep"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toStartOf="@+id/buttonLimpar"
			android:layout_marginStart="8dp"/>
	<Button
			android:text="Limpar"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:id="@+id/buttonLimpar"
			app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="8dp"
			app:layout_constraintTop_toTopOf="@+id/buttonSalvar"
			app:layout_constraintStart_toEndOf="@+id/buttonSalvar"
			android:layout_marginStart="8dp"/>
</android.support.constraint.ConstraintLayout>
